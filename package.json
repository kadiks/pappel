{
  "name": "pappel",
  "version": "0.1.2",
  "description": "Babbel for apps. Localization library",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "pappel": "./bin/index.js"
  },
  "scripts": {
    "test": "npm run babel-build ; ./node_modules/mocha/bin/mocha test/unit/ --recursive -R min || true",
    "test-keys": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/Keys.js || true",
    "test-vars": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/Vars.js || true",
    "test-xlsx2p": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converter/XLSX2Pappel.js || true",
    "test-android2p": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converter/AndroidXML2Pappel.js || true",
    "test-strings2p": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converter/Strings2Pappel.js || true",
    "test-p2strings": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converter/Pappel2Strings.js || true",
    "test-p2android": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converter/Pappel2AndroidXML.js || true",
    "test-p2rnloc": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converter/Pappel2ReactNativeLocalization.js || true",
    "babel-build": "./node_modules/babel-cli/bin/babel.js ./lib --out-dir ./build",
    "doc": "npm run babel-build ; jsduck --config docs/config.json; cp docs/monochrome-white-jsduck.png ../pappel-doc/resources/images/logo.png ; cp docs/favicon.ico ../pappel-doc/favicon.ico",
    "cov": "npm run precov ; PAPPEL_COV=1 ./node_modules/mocha/bin/mocha test/unit/ --recursive -R html-cov > ./test/output/coverage.html || true ; npm run postcov",
    "precov": "jscover --no-highlight ./build/ ./build-cov/",
    "postcov": "rm -rf ./build-cov/",
    "release": "npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadiks/pappel.git"
  },
  "keywords": [
    "localization",
    "internationalization",
    "l10n",
    "i18n",
    "xml",
    "xliff",
    "csv",
    "xsl",
    "xslx",
    "json",
    "strings",
    "iOS",
    "Android",
    "react",
    "react-native"
  ],
  "author": "Jénaïc Cambré <me@jenaiccambre.com> (www.jenaiccambre.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kadiks/pappel/issues"
  },
  "homepage": "https://github.com/kadiks/pappel#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "jscover": "^1.0.0",
    "jscoverage": "^0.6.0",
    "mocha": "^2.4.5",
    "should": "^8.3.1"
  },
  "dependencies": {
    "i18n-strings-files": "^1.0.2",
    "minimist": "^1.2.0",
    "skz-logger": "git://github.com/kadiks/skz-logger.git",
    "xlsx": "^0.8.0",
    "xml-parser": "^1.2.1"
  }
}
