{
  "name": "pappel",
  "version": "0.1.0",
  "description": "Babbel for apps. Localization library",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run babel-build ; ./node_modules/mocha/bin/mocha test/unit/ --recursive -R min || true",
    "test-keys": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/Keys.js || true",
    "test-xlsx2p": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converters/XLSX2Pappel.js || true",
    "test-p2strings": "npm run babel-build ; ./node_modules/mocha/bin/mocha ./test/unit/converters/Pappel2Strings.js || true",
    "babel-build": "./node_modules/babel-cli/bin/babel.js ./lib --out-dir ./build",
    "doc": "npm run babel-build ; jsduck --config jsduck.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kadiks/pappel.git"
  },
  "keywords": [
    "localization",
    "internationalization",
    "l10n",
    "i18n",
    "xml",
    "xliff",
    "csv",
    "xsl",
    "xslx",
    "json",
    "strings",
    "iOS",
    "Android",
    "react",
    "react-native"
  ],
  "author": "Jénaïc Cambré <me@jenaiccambre.com> (www.jenaiccambre.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kadiks/pappel/issues"
  },
  "homepage": "https://github.com/kadiks/pappel#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "jscover": "^1.0.0",
    "jscoverage": "^0.6.0",
    "mocha": "^2.4.5",
    "should": "^8.3.1"
  },
  "dependencies": {
    "xlsx": "^0.8.0",
    "skz-logger": "git://github.com/kadiks/skz-logger.git"
  }
}
